{
  "compilerOptions": {
    "outDir": "app/dist/js", //compilar em js para a pasta dist
    "target": "ES6",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "removeComments": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": ["app/**/*"]
}
